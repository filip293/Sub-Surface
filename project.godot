config_version=5

[application]

config/name="Sub-Surface"
run/main_scene="res://Scripts/Sub_Surface.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globals="*res://Scripts/Globals.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[debug]

settings/stdout/print_fps=true

[dialogue_manager]

runtime/balloon_path="res://Dialogue/balloon.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/vsync/vsync_mode=0

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/post_processing/plugin.cfg")

